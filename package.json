  "author": "anchan828 <anchan828@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "compodoc": "compodoc -c .compodocrc.json",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "postinstall": "husky install",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:major": "lerna publish --yes major",
    "publish:minor": "lerna publish --yes minor",
    "test": "turbo run test",
    "watch": "turbo run watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/config-lerna-scopes": "^17.0.2",
    "@compodoc/compodoc": "^1.1.19",
    "@lerna-lite/cli": "^1.11.3",
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/testing": "^9.1.2",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.1.0",
    "@types/node": "^18.7.23",
    "@types/prettier": "^2.7.1",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "jest": "^29.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "turbo": "^1.5.4",
    "typescript": "^4.8.4"
  }
